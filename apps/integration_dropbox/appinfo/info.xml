<?xml version="1.0"?>
<info>
    <id>integration_dropbox</id>
    <name>Dropbox integration</name>
    <summary>Import Dropbox data in Nextcloud</summary>
    <description><![CDATA[Dropbox integration allows you to automatically import your Dropbox files into Nextcloud.]]></description>
    <version>1.0.4</version>
    <licence>agpl</licence>
    <author>Julien Veyssier</author>
    <namespace>Dropbox</namespace>
    <documentation>
        <developer>https://github.com/eneiluj/integration_dropbox/wikis</developer>
    </documentation>
    <category>integration</category>
    <website>https://github.com/eneiluj/integration_dropbox</website>
    <bugs>https://github.com/eneiluj/integration_dropbox/issues</bugs>
    <screenshot>https://github.com/eneiluj/integration_dropbox/raw/master/img/screenshot1.jpg</screenshot>
    <dependencies>
        <nextcloud min-version="22" max-version="24"/>
    </dependencies>
    <settings>
        <admin>OCA\Dropbox\Settings\Admin</admin>
        <admin-section>OCA\Dropbox\Settings\AdminSection</admin-section>
        <personal>OCA\Dropbox\Settings\Personal</personal>
        <personal-section>OCA\Dropbox\Settings\PersonalSection</personal-section>
    </settings>
</info>
